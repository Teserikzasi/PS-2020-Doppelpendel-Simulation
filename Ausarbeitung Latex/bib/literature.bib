@Manual{pgfplots,
  title    = {Manual for Package PGFPLOTS -- 2D/3D Plots in \LaTeX, Version 1.5.1},
  author   = {Feuersänger, Christian},
  month    = {12},
  year     = {2011},
  abstract = {pgfplots draws high-quality function plots in normal or logarithmic
	scaling with a user-friendly
	
	interface directly in TEX. The user supplies axis labels, legend entries
	and the plot coordinates for one
	
	or more plots and pgfplots applies axis scaling, computes any logarithms
	and axis ticks and draws the
	
	plots. It supports line plots, scatter plots, piecewise constant plots,
	bar plots, area plots, mesh- and
	
	surface plots, patch plots, contour plots, quiver plots, histogram
	plots, polar axes, ternary diagrams,
	
	smith charts and some more. It is based on Till Tantau's package pgf/TikZ.},
}

